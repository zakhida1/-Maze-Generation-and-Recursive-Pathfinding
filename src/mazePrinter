// MazePrinter.java
public class MazePrinter {
    public static void printMaze(Maze maze) {
        for (int r = 0;r < maze.grid.length; r++) {
            for (int c=0; c < maze.grid[0].length; c++) {
                System.out.print(maze.grid[r][c]);
            }
            System.out.println();
        }
    }
}

